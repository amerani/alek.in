{
  "name": "alek.in",
  "version": "1.0.10",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./src/index.html ./src/error.html",
    "build": "parcel build ./src/index.html ./src/error.html --no-source-maps",
    "zip": "zip -r lambda.zip src/lambda",
    "clean": "rm -rf dist",
    "clean:s3": "aws s3 rm s3://alek --recursive",
    "clean:zip": "rm -f lambda.zip",
    "patch": "npm version patch",
    "publish:s3": "aws s3 cp dist/ s3://alek --recursive",
    "publish:lambda": "aws lambda update-function-code --function-name alek-in-proxy --zip-file fileb://lambda.zip --publish",
    "publish": "npm run patch && npm run clean && npm run clean:s3 && npm run build && npm run publish:s3 && npm run zip && npm run publish:lambda && npm run clean:zip"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "aws-sdk": "^2.311.0",
    "babel-preset-react-app": "^3.1.0",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "fs-jetpack": "^2.1.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "latest",
    "simple-icons": "^1.8.7"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amerani/alek.in.git"
  },
  "bugs": {
    "url": "https://github.com/amerani/alek.in/issues"
  },
  "homepage": "https://github.com/amerani/alek.in#readme"
}
